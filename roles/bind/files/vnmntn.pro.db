$TTL 86400
@ IN SOA dns-primary.vnmntn.pro. admin.vnmntn.pro. (
                                                2020011800 ;Serial
                                                3600 ;Refresh
                                                1800 ;Retry
                                                604800 ;Expire
                                                86400 ;Minimum TTL
)

;Name Server Information
