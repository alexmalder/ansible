[Unit]
Description=Jupyter Notebook

[Service]
Type=simple
#PIDFile=/run/jupyter.pid
ExecStart=jupyter notebook --config=/home/jupyter/.jupyter/jupyter_notebook_config.py --ip=10.12.10.1 --port=8888
User=jupyter
Group=jupyter
WorkingDirectory=/home/jupyter/notebooks/
Restart=always
RestartSec=10
#KillMode=mixed

[Install]
WantedBy=multi-user.target
